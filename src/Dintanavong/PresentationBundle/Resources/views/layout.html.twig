<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}DINTANAVONG{% endblock %}</title>
    <link rel="stylesheet" type="text/css" href="{{ asset('materialize/css/materialize.min.css') }}"/>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('style.css') }}"/>
</head>
<body>
{% block body %}
    <div class="row" style="margin: 0 !important;">
        <div class="col s12">
            <!-- Menu -->
            <nav class="nav-extended black">
                <div class="nav-content">
                    <ul class="tabs tabs-transparent">
                        <li class="tab"><a href="#accueil">Accueil</a></li>
                        <li class="tab"><a href="#services">Services</a></li>
                        <li class="tab"><a href="#aproposdemoi">A propos de moi</a></li>
                        <li class="tab"><a href="#mesprojets">Mes projets</a></li>
                        <li class="tab"><a href="#contact">Contact</a></li>
                        <li class="tab right"><a href="#seconnecter" data-activates="slide-out" class="se_connecter"><i class="material-icons">menu</i></a></li>
                    </ul>
                </div>
            </nav>
        </div>
    </div>
    <!-- Sidebar -->
    <div class="row">
        <div class="col s12">
            <ul id="slide-out" class="side-nav">
                <li><div class="user-view">
                        <div class="background">
                            <img src="https://static.pexels.com/photos/145939/pexels-photo-145939.jpeg">
                        </div>
                        <a href="#!user"><img class="circle" src="data:image/jpeg;base64,"></a>
                        <a href="#!name"><span class="white-text name">John Doe</span></a>
                        <a href="#!email"><span class="white-text email">jdandturk@gmail.com</span></a>
                    </div></li>
                <li><a href="#!"><i class="material-icons">account_circle</i>Se connecter</a></li>
                <li><div class="divider"></div></li>
                <li><a class="subheader">Plus d'informations</a></li>
                <li><a class="waves-effect" href="#!"><i class="material-icons">account_balance</i>Charte informatique</a></li>
            </ul>
            <!-- Different content when we click on tab panel -->
            <div id="accueil" class="col s12"></div>
            <div id="services" class="col s12"></div>
            <div id="aproposdemoi" class="col s12"></div>
            <div id="mesprojets" class="col s12"></div>
            <div id="contact" class="col s12"></div>
        </div>
    </div>
{% endblock %}
<script src="{{ asset('jquery-3.2.1.min.js') }}"></script>
<script src="{{ asset('materialize/js/materialize.min.js') }}"></script>
<script src="{{ asset('script.js') }}"></script>
</body>
</html>